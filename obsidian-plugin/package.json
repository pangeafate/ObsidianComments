{
  "name": "obsidian-comments",
  "version": "2.1.1",
  "description": "Share notes online for collaborative editing with comments",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:single": "jest --testNamePattern",
    "tdd:start": "npm run test:watch",
    "tdd:complete": "npm run test:coverage && npm run lint",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "verify-tdd": "node scripts/verify-tdd.js",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": ["obsidian", "plugin", "sharing", "collaboration", "comments"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.17.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}